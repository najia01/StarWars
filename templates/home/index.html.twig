{% extends 'base.html.twig' %}

{% block title %}Index des personnages
{% endblock %}

{% block body %}
	{% for personnage in personnages %}

		<article>
            <a href="{{ path('app_personnage', {'id': (personnage.url|split('/'))[5]}) }}">
            {{ personnage.name }}
        </a>
		</article>
	{% endfor %}
{% endblock %}
